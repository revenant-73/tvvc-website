  <script>
    // Module Navigation
    function showModule(moduleId) {
      // Hide all modules
      document.querySelectorAll('.course-module').forEach(module => {
        module.style.display = 'none';
      });
      
      // Show selected module
      document.getElementById(moduleId).style.display = 'block';
      
      // Update active tab
      document.querySelectorAll('.category-tab').forEach(tab => {
        tab.classList.remove('active');
        tab.style.backgroundColor = 'rgba(30, 30, 30, 0.7)';
      });
      
      document.getElementById('nav-' + moduleId).classList.add('active');
      document.getElementById('nav-' + moduleId).style.backgroundColor = 'var(--color-coral)';
      
      // Scroll to top of module
      window.scrollTo({
        top: document.querySelector('.section-title').offsetTop - 100,
        behavior: 'smooth'
      });
    }
    
    // Tab Navigation
    function activateTab(button, tabId) {
      // Hide all tab panes
      document.querySelectorAll('.tab-pane').forEach(tab => {
        tab.style.display = 'none';
        tab.classList.remove('active');
      });
      
      // Show selected tab pane
      document.getElementById(tabId).style.display = 'block';
      document.getElementById(tabId).classList.add('active');
      
      // Update active tab button
      document.querySelectorAll('.tab-btn').forEach(btn => {
        btn.classList.remove('active');
        btn.style.backgroundColor = 'rgba(30, 30, 30, 0.7)';
      });
      
      button.classList.add('active');
      button.style.backgroundColor = 'var(--color-teal)';
    }
    
    // Next/Previous Module Navigation
    document.addEventListener('DOMContentLoaded', function() {
      // Next module buttons
      document.querySelectorAll('.next-module-btn').forEach(button => {
